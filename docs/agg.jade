extends _docs.tpl.jade

block append vars
  - var page = ConfDocs.DOCS.find('agg.html')

block append main
  +newSection(2, ConfDocs.DOCS.find('agg.html#site-header'))
    include _includes/agg.site-header.docs.jade
  +newSection(2, ConfDocs.DOCS.find('agg.html#page-header'))
    include _includes/agg.page-header.docs.jade
  +newSection(2, ConfDocs.DOCS.find('agg.html#highlighted-content-block'))
    include _includes/agg.highlighted-content-block.docs.jade
  +newSection(2, ConfDocs.DOCS.find('agg.html#hero-image'))
    include _includes/agg.hero-image.docs.jade
  +newSection(2, ConfDocs.DOCS.find('agg.html#program'))
    include _includes/agg.program.docs.jade
  +newSection(2, ConfDocs.DOCS.find('agg.html#speaker'))
    include _includes/agg.speaker.docs.jade
  +newSection(2, ConfDocs.DOCS.find('agg.html#important-dates'))
    include _includes/agg.important-dates.docs.jade
  +newSection(2, ConfDocs.DOCS.find('agg.html#place'))
    include _includes/agg.place.docs.jade

block append scripts
  script(src="/scripts/c-Program.js")
  script.
    $('.c-Toggle__Check').change(function () {
      $(this).parents('.c-Toggle').siblings('.o-Drawer').toggleClass('o-Drawer--open')
    })
