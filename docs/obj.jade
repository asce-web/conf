extends _docs.tpl.jade

block append vars
  - var page = ConfDocs.DOCS.find('obj.html')

block append main
  p.
    This style guide imports styles from the Xmeter stylesheet.
    See the
    #[a(href="https://cdn.rawgit.com/chharvey/xmeter/master/docs/obj.html") Xmeter style guide]
    for more information.
  +newSection(2, ConfDocs.DOCS.find('obj.html#float-grid'))
    include _includes/o-GridFloat.docs.jade
  +newSection(2, ConfDocs.DOCS.find('obj.html#runner-object'))
    include _includes/o-Runner.docs.jade
  +newSection(2, ConfDocs.DOCS.find('obj.html#constrain'))
    include _includes/o-Constrain.docs.jade
  +newSection(2, ConfDocs.DOCS.find('obj.html#action-list'))
    include _includes/o-ListAction.docs.jade
  +newSection(2, ConfDocs.DOCS.find('obj.html#stacked-list'))
    include _includes/o-ListStacked.docs.jade
  +newSection(2, ConfDocs.DOCS.find('obj.html#drawer-object'))
    include _includes/o-Drawer.docs.jade

block append scripts
  script.
    $('#drawer-check').change(function () {
      $(this).parents('.docs-c-Togglable').siblings('.o-Drawer').toggleClass('o-Drawer--open')
    })
