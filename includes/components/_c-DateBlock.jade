mixin dateBlock(all_dates)
  dl.c-DateBlock
    each action in all_dates
      unless action.isHidden()
        dt.c-DateBlock__Date
          time(datetime=action.startTime)
            - var date = Util.toDate(action.startTime)
            = Util.monthDay(date) + ', ' + date.getFullYear()
        dd.c-DateBlock__Desc
          if action.getURL()
            a.c-DateBlock__Link.h-Block(href=action.getURL())
              span= action.name
              i.material-icons arrow_forward
          else
            = action.name
