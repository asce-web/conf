      pre.docs-pre.
        &lt;div class="c-Banner"&gt;
          &lt;h1 class="#[u c-PageTitle]  #[u c-PageTitle--special]"&gt;special page title&lt;/h1&gt;
        &lt;/div&gt;
  section.sg-sect#banner-component
    h2 The Banner Component
    figure.docs-figure
      div.c-Banner(style="margin-bottom: 1.5rem; z-index:1;")
        h1 banner heading
        p banner content
      pre.docs-pre.
        &lt;div class="#[u c-Banner]"&gt;
          &lt;h1&gt;banner heading&lt;/h1&gt;
          &lt;p&gt;banner content&lt;/p&gt;
        &lt;/div&gt;
  section.sg-sect#hero-block
    h2 The Hero Block
    figure.docs-figure
      -
        var demoevent = {
          name      : 'Geo-Risk 2017'
        , theme     : 'Geotechincal risk from theory to practice.'
        , startDate : '2017-06-04'
        , endDate   : '2017-06-07'
        , url       : 'http://2017.georiskconference.org/'
        , location  : {
            addressLocality     : 'Denver'
          , addressRegion       : 'CO'
          , addressRegion_title : 'Colorado'
          }
        }
      //- div.c-Hero
        +hero-content(demoevent)
