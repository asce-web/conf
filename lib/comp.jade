extends lib.tpl.jade

block append vars
  - var page = Lib.getPage('comp.html')

block toc
  +libtoc({pages: page.sub, depth: Infinity})

block append main
  section.sg-sect#masthead-mastfoot
    h2 The Masthead and Mastfoot
    figure.docs-figure
      header.c-Mast
        h1 mast heading
        p mast content
        ul
          li: a(href="") mast link
          li: a(href="")._hover mast link hover
          li: a(href="")._active mast link active
          li: a mast link disabled
      pre.docs-pre.
        &lt;header class="#[u c-Mast]"&gt;
          &lt;h1&gt;mast heading&lt;/h1&gt;
          &lt;p&gt;mast content&lt;/p&gt;
          &lt;p&gt;&lt;a&gt;mast link&lt;/a&gt;&lt;/p&gt;
        &lt;/header&gt;
    figure.docs-figure
      footer.c-Mast.c-Mast--copyright
        ul
          li: a(href="") mast link
          li: a(href="")._hover mast link hover
          li: a(href="")._active mast link active
          li: a mast link disabled
      pre.docs-pre.
        &lt;footer class="#[u c-Mast]  #[u c-Mast--copyright]"&gt;
          &lt;p&gt;&lt;a&gt;&copy; 1996&ndash;2016 American Society of Civil Engineers&lt;/a&gt;&lt;/p&gt;
        &lt;/footer&gt;
    figure.docs-figure
      footer.c-Mast.c-Mast--internal
        ul
          li: a(href="") mast link
          li: a(href="")._hover mast link hover
          li: a(href="")._active mast link active
          li: a mast link disabled
      pre.docs-pre.
        &lt;footer class="#[u c-Mast]  #[u c-Mast--internal]"&gt;
          &lt;p&gt;&lt;a&gt;&copy; 1996&ndash;2016 American Society of Civil Engineers&lt;/a&gt;&lt;/p&gt;
        &lt;/footer&gt;
  section.sg-sect#site-title
    h2 The Site Title
    p Use with #[a(href="#text-level-components") the Camo Link].
    figure.docs-figure
      a.o-Block.o-GridFloat.c-SiteTitle.c-Camo(href="" style="margin-bottom: 1.5rem;")
        span.o-GridFloat__Item.c-SiteTitle__Logo
        h1.o-GridFloat__Item.c-SiteTitle__Name ASCE Convention
        p.o-GridFloat__Item.c-SiteTitle__Tagline The Flagship Event of ASCE
      pre.docs-pre.
        &lt;a class="o-Block  o-GridFloat  #[u c-SiteTitle]  c-Camo"&gt;
          &lt;span class="o-GridFloat__Item  #[u c-SiteTitle__Logo]"&gt;&lt;/span&gt;
          &lt;h1 class="o-GridFloat__Item  #[u c-SiteTitle__Name]"&gt;ASCE Convention&lt;/h1&gt;
          &lt;p class="o-GridFloat__Item  #[u c-SiteTitle__Tagline]"&gt;The Flagship Event of ASCE&lt;/p&gt;
        &lt;/a&gt;
  section.sg-sect#universal-menu
    h2 The Universal Menu
    p #[strong Note:] Universal Menu will convert to pull-out drawer when screen is smaller than 720px.
    p Current Year menu item will appear exactly the same as Hero Button, but will not have that class.
    figure.docs-figure
      -
        var pages1 = [
          { name: 'About'  , href: '' }
        , { name: 'Sponsor', href: '' }
        , { name: 'Exhibit', href: '' }
        , { name: 'Contact', href: '' }
        ]
        var pages2 = [
          { name: 'Home', href: '' }
        ]
      div(style="margin-bottom: 1.5rem;")
        include /includes/components/_c-MenuUniversal.jade
        +menuUniversal(pages1, pages2, 'ASCE 2016')
      ul.o-ListBare.o-GridFlex.c-MenuUniversal(style="margin-bottom: 1.5rem;")
        li.o-GridFlex__Item.c-MenuUniversal__Item: a.o-Block.c-MenuUniversal__Link(href="")._hover menu item hover
        li.o-GridFlex__Item.c-MenuUniversal__Item: a.o-Block.c-MenuUniversal__Link(href="")._active menu item active
        li.o-GridFlex__Item.c-MenuUniversal__Item: a.o-Block.c-MenuUniversal__Link menu item disabled
        li.o-GridFlex__Item.c-MenuUniversal__Item: a.o-Block.c-MenuUniversal__Link.c-MenuUniversal__Link--current(href="")._hover current year hover
        li.o-GridFlex__Item.c-MenuUniversal__Item: a.o-Block.c-MenuUniversal__Link.c-MenuUniversal__Link--current(href="")._active current year active
      pre.docs-pre.
        &lt;ul class="o-ListBare  o-GridFlex  #[u c-MenuUniversal]"&gt;
          &lt;li class="o-GridFlex__Item  #[u c-MenuUniversal__Item]"&gt;
            &lt;a class="o-Block  #[u c-MenuUniversal__Link]"&gt;About&lt;/a&gt;
          &lt;/li&gt;
          ...
          &lt;li class="o-GridFlex__Item  #[u c-MenuUniversal__Item]"&gt;
            &lt;a class="o-Block  #[u c-MenuUniversal__Link]  #[u c-MenuUniversal__Link--current]"&gt;ASCE 2016&lt;/a&gt;
          &lt;/li&gt;
        &lt;/ul&gt;
  section.sg-sect#standard-menu
    h2 The Standard Menu
    figure.docs-figure
      include /sites/all/includes/_data.jade
      -
        var pages = [
          { name: 'Home'          , href: '' }
