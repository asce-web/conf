section#site-title
  h2 The Site Title
  p Use with #[a(href="#camo-link") the Camo Link].
  figure.docs-figure
    include /includes/components/_c-SiteTitle.jade
    +siteTitle(new ConfSite('CI Summit', 'http://cisummit.org/sites/cisummit.org/files/ci-shield-logo.png')
      .setDescription('Innovations In Construction')
      .init())
    pre.docs-pre.
      &lt;a class="o-GridFloat  #[u c-SiteTitle]  c-Camo  h-Block"&gt;
        &lt;span class="o-GridFloat__Item  #[u c-SiteTitle__Logo]"&gt;&lt;/span&gt;
        &lt;h1 class="o-GridFloat__Item  #[u c-SiteTitle__Name]"&gt;CI Summit&lt;/h1&gt;
        &lt;p class="o-GridFloat__Item  #[u c-SiteTitle__Tagline]"&gt;Innovations In Construction&lt;/p&gt;
      &lt;/a&gt;
