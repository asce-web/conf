{
  "name": "conf",
  "version": "0.2.0",
  "description": "The official ASCE conference site style guide.",
  "main": "index.html",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "jade:index": "pug index.jade",
    "jade:docs": "pug -O docs/models/options.js docs/{index,visual,base,obj,comp,help,atom}.jade",
    "jade:sites": "pug -O sites/all/models/options.js sites/all/{index,about,sponsor,exhibit,contact,home,registration,program,location,speakers}.jade",
    "jade:default": "pug -O sites/default/models/options.js sites/default/{index,about,sponsor,exhibit,contact,home,registration,program,location,speakers}.jade",
    "jade:all": "npm run jade:index  &&  npm run jade:docs  &&  npm run jade:sites  &&  npm run jade:default",
    "lessc": "lessc",
    "lessc:conf": "lessc styles/conf.less styles/conf.css",
    "lessc:docs": "lessc docs/styles/docs.less docs/styles/docs.css",
    "lessc:default": "lessc sites/default/styles/site.less sites/default/styles/site.css",
    "lessc:all": "npm run lessc:conf  &&  npm run lessc:docs  &&  npm run lessc:default",
    "minify": "cleancss styles/conf.css -o styles/conf.min.css",
    "browserify:visual": "browserify --no-commondir docs/scripts/visual.js > docs/scripts/visual.b.js",
    "browserify:home": "browserify --no-commondir sites/default/scripts/home.js > sites/default/scripts/home.b.js",
    "browserify:all": "npm run browserify:visual && npm run browserify:home",
    "unbuild:root": "rm index.html docs/{index,visual,base,obj,comp,help,atom}.html",
    "unbuild:sites": "rm sites/all/{index,about,sponsor,exhibit,contact,home,registration,program,location,speakers}.html",
    "unbuild:default": "rm sites/default/{index,about,sponsor,exhibit,contact,home,registration,program,location,speakers}.html",
    "unbuild:css": "rm styles/conf.css styles/conf.min.css docs/styles/docs.css sites/default/styles/site.css",
    "unbuild": "npm run unbuild:root  &&  npm run unbuild:sites  &&  npm run unbuild:default  &&  npm run unbuild:css",
    "build": "npm run jade:all  &&  npm run lessc:all  &&  npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asce-web/conf.git"
  },
  "author": "ASCE Web Team <webteam@asce.org> (https://github.com/asce-web/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asce-web/conf/issues"
  },
  "homepage": "http://conf.style.asce.org/",
  "devDependencies": {
    "browserify": "^13.1.0",
    "clean-css": "^3.4.9",
    "csscolor": "^1.0.0",
    "jade": "^1.11.0",
    "less": "^2.6.0",
    "pug-cli": "^1.0.0-alpha6",
    "xmeter": "^5.0.0"
  },
  "dependencies": {}
}
