{
  "name": "conf",
  "version": "0.2.0",
  "description": "The official ASCE conference site style guide.",
  "main": "index.html",
  "private": true,
  "scripts": {
    "jade": "jade",
    "jade:sg": "jade {index,coding,writing,visual,struct}.jade",
    "jade:lib": "jade lib/{lib.tpl,index,base,obj,comp-typo,comp-ui,helpers,atoms}.jade",
    "jade:sample": "jade sample/{sample.tpl,index,home,int}.jade",
    "jade:all": "npm run jade:sg  ;  npm run jade:lib  ;  npm run jade:sample",
    "lessc": "lessc",
    "lessc:conf": "lessc styles/conf.less styles/conf.css",
    "lessc:sg": "lessc styles/styleguide.less styles/styleguide.css",
    "lessc:all": "npm run lessc:conf  ;  npm run lessc:sg",
    "minify": "cleancss styles/conf.css -o styles/conf.min.css",
    "build": "npm run jade:all  ;  npm run lessc:all  ;  npm run minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asce-web/conf.git"
  },
  "author": "ASCE Web Team <webteam@asce.org> (https://github.com/asce-web/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asce-web/conf/issues"
  },
  "homepage": "http://conf.style.asce.org/",
  "devDependencies": {
    "bangs": "^0.7.0",
    "clean-css": "^3.4.9",
    "harp": "^0.20.1",
    "jade": "^1.11.0",
    "less": "^2.6.0",
    "xmeter": "^4.0.0"
  },
  "dependencies": {}
}
