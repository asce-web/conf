{
  "name": "conf",
  "version": "0.3.0",
  "description": "The official ASCE conference site style guide.",
  "main": "index.html",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pug:index": "pug index.jade",
    "pug:docs": "pug -O docs/_models/options.js docs/{index,principles,base,obj,comp,temp,help,atom}.jade",
    "pug:sites": "pug -O sites/all/_models/options.js sites/all/{index,registration,program,location,speakers,sponsor,exhibit,about,contact}.jade",
    "pug:default": "pug -O sites/default/_models/options.js sites/default/{index,registration,program,location,speakers,sponsor,exhibit,about,contact}.jade",
    "pug:all": "npm run pug:index  &&  npm run pug:docs  &&  npm run pug:sites  &&  npm run pug:default",
    "lessc": "lessc",
    "lessc:conf": "lessc styles/conf.less styles/conf.css",
    "lessc:docs": "lessc docs/styles/docs.less docs/styles/docs.css",
    "lessc:default": "lessc sites/default/styles/site.less sites/default/styles/site.css",
    "lessc:all": "npm run lessc:conf  &&  npm run lessc:docs  &&  npm run lessc:default",
    "minify": "cleancss styles/conf.css -o styles/conf.min.css",
    "browserify:principles": "browserify docs/scripts/principles.js > docs/scripts/principles.b.js",
    "browserify:home": "browserify sites/default/scripts/home.js > sites/default/scripts/home.b.js",
    "browserify:all": "npm run browserify:principles && npm run browserify:home",
    "unbuild:root": "rm index.html docs/{index,principles,base,obj,comp,temp,help,atom}.html",
    "unbuild:sites": "rm sites/all/{index,registration,program,location,speakers,sponsor,exhibit,about,contact}.html",
    "unbuild:default": "rm sites/default/{index,registration,program,location,speakers,sponsor,exhibit,about,contact}.html",
    "unbuild:css": "rm styles/conf.css styles/conf.min.css docs/styles/docs.css sites/default/styles/site.css",
    "unbuild:js": "rm docs/scripts/principles.b.js sites/default/scripts/home.b.js",
    "unbuild": "npm run unbuild:root  &&  npm run unbuild:sites  &&  npm run unbuild:default  &&  npm run unbuild:css  &&  npm run unbuild:js",
    "build": "npm run pug:all  &&  npm run lessc:all  &&  npm run minify  &&  npm run browserify:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asce-web/conf.git"
  },
  "author": "ASCE Web Team <webteam@asce.org> (https://github.com/asce-web/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asce-web/conf/issues"
  },
  "homepage": "http://conf.style.asce.org/",
  "devDependencies": {
    "browserify": "^13.1.0",
    "clean-css": "^3.4.9",
    "csscolor": "^1.0.0",
    "jade": "^1.11.0",
    "less": "^2.6.0",
    "pug-cli": "^1.0.0-alpha6",
    "sitepage": "^1.0.0",
    "xmeter": "^5.0.0"
  },
  "dependencies": {}
}
