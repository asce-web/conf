{
  "name": "conf",
  "version": "0.2.0",
  "description": "The official ASCE conference site style guide.",
  "main": "index.html",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "jade": "jade --obj {basedir:\\\"./\\\"}",
    "jade:index": "npm run jade index.jade",
    "jade:lib": "npm run jade lib/{index,base,obj,comp,comp-typo,comp-ui,helpers,atoms}.jade",
    "jade:sample": "npm run jade sample/{index,about,sponsor,exhibit,contact,home,registration,program,location,speakers}.jade",
    "jade:all": "npm run jade:index  &&  npm run jade:lib  &&  npm run jade:sample",
    "lessc": "lessc",
    "lessc:conf": "lessc styles/conf.less styles/conf.css",
    "lessc:lib": "lessc lib/styles/lib.less lib/styles/lib.css",
    "lessc:sample": "lessc sites/default/styles/site.less sites/default/styles/site.css",
    "lessc:all": "npm run lessc:conf  &&  npm run lessc:lib  &&  npm run lessc:sample",
    "minify": "cleancss styles/conf.css -o styles/conf.min.css",
    "build": "npm run jade:all  &&  npm run lessc:all  &&  npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asce-web/conf.git"
  },
  "author": "ASCE Web Team <webteam@asce.org> (https://github.com/asce-web/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asce-web/conf/issues"
  },
  "homepage": "http://conf.style.asce.org/",
  "devDependencies": {
    "clean-css": "^3.4.9",
    "jade": "^1.11.0",
    "less": "^2.6.0",
    "xmeter": "^4.0.0"
  },
  "dependencies": {}
}
