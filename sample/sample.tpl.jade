doctype html
html(lang="en")
  head
    block vars
      - var title = ''
      - var conf_name = 'ASCE 2016'
    title #{title} | Sample ASCE Conference Site
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel="stylesheet" href="../node_modules/xmeter/xmeter.min.css")
    link(rel="stylesheet" href="../node_modules/bangs/bangs.min.css")
    link(rel="stylesheet" href="../styles/conf.css")
    block style
      style.
        section.this-is-editable { background-color: rgba(255, 219, 64, 0.64); }
  body
    mixin action-buttons()
      ul.o-ListBare.o-GridFlex.-xj-c.-ta-c
        li.o-GridFlex__Item.-w-a-sM: a(href="") Registor Today
        li.o-GridFlex__Item.-w-a-sM: a(href="") Become A Sponsor
        li.o-GridFlex__Item.-w-a-sM: a(href="") Submit Papers
    block front-matter
      header.o-GridFlex.c-Mast.-xj-sb.o-BodyChild
        a.o-GridFlex__Item.o-Block.c-Mast__Title.-w-a(href="")
          h1 ASCE Convention
          p Tagline. The Flagship Event for ASCE
        div.o-GridFlex__Item.-w-a.-d-n-sM: button menu
        nav.o-GridFlex__Item.o-GridFlex.c-Mast__Nav.-xc-xs.-w-10em.-w-a-sM
          h2.o-GridFlex__Item.c-Mast__Nav__Hn.-d-n-sM Menu
          ul.o-GridFlex__Item.o-ListBare.o-GridFlex.c-Mast__Nav__Menu.-w-a-sM
            li.o-GridFlex__Item.c-Mast__Nav__Menu__Item.-w-a-sM: a.o-Block.c-Mast__Nav__Menu__Link(href="") About
            li.o-GridFlex__Item.c-Mast__Nav__Menu__Item.-w-a-sM: a.o-Block.c-Mast__Nav__Menu__Link(href="") Sponsor
            li.o-GridFlex__Item.c-Mast__Nav__Menu__Item.-w-a-sM: a.o-Block.c-Mast__Nav__Menu__Link(href="") Exhibit
            li.o-GridFlex__Item.c-Mast__Nav__Menu__Item.-w-a-sM: a.o-Block.c-Mast__Nav__Menu__Link(href="") Contact
          div.o-GridFlex__Item.c-Mast__Nav__Hn.-w-a-sM: a.o-Block(href="") 2016 Convention
      unless page_type === 'landing'
        nav
          ul.o-ListBare.o-GridFlex.c-NavMenu.-mb-0.o-BodyChild
            li.o-GridFlex__Item.c-NavMenu__Item.-w-a
              a.o-Block.c-NavMenu__Link(href="") Home
            li.o-GridFlex__Item.c-NavMenu__Item.-w-a
              a.o-Block.c-NavMenu__Link(href="") Register
              ul.o-ListBare.c-NavSubmenu(class=(title==='Register' ? 'c-NavSubmenu--active' : ''))
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Why Attend
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Register Submenu Item 2
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Register Submenu Item 3
            li.o-GridFlex__Item.c-NavMenu__Item.-w-a
              a.o-Block.c-NavMenu__Link(href="") Program
              ul.o-ListBare.c-NavSubmenu(class=(title==='Program' ? 'c-NavSubmenu--active' : ''))
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Events
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Program Submenu Item 2
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Program Submenu Item 3
            li.o-GridFlex__Item.c-NavMenu__Item.-w-a
              a.o-Block.c-NavMenu__Link(href="") Location
              ul.o-ListBare.c-NavSubmenu(class=(title==='Interior Page' ? 'c-NavSubmenu--active' : ''))
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") International Travel
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Location Submenu Item 2
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") Location Submenu Item 3
            li.o-GridFlex__Item.c-NavMenu__Item.-w-a
              a.o-Block.c-NavMenu__Link(href="") Call for Submissions
              ul.o-ListBare.c-NavSubmenu(class=(title==='Call for Submissions' ? 'c-NavSubmenu--active' : ''))
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") CfS Submenu Item 1
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") CfS Submenu Item 2
                li.c-NavSubmenu__Item: a.o-Block.c-NavSubmenu__Link(href="") CfS Submenu Item 3
    main.o-BodyChild
      block main
    block end-matter
      footer.o-GridFlex.c-Mast.o-BodyChild(itemscope="" itemtype="http://schema.org/NGO")
        section.o-GridFlex__Item.-w-1o2-sM
          h1 Contact Us
          address(itemprop="contactPoint" itemscope="" itemtype="http://schema.org/ContactPoint"): ul.o-ListBare
            li: a(href="tel:8005482723" itemprop="telephone") (800) 548-2723
            li: a(href="tel:17032956300" itemprop="telephone") +1 (703) 295-6300
        section.o-GridFlex__Item.-w-1o2-sM
          h1 Connect With ASCE
          ul.o-ListBare.o-GridFlex
            li.o-GridFlex__Item.-w-a: a(href="" itemprop="sameAs") LinkedIn
            li.o-GridFlex__Item.-w-a: a(href="" itemprop="sameAs") Facebook
            li.o-GridFlex__Item.-w-a: a(href="" itemprop="sameAs") Twitter
            li.o-GridFlex__Item.-w-a: a(href="" itemprop="sameAs") YouTube
            li.o-GridFlex__Item.-w-a: a(href="" itemprop="sameAs") Google+
        div.o-GridFlex__Item.c-Copyright
          a.o-Block.c-Copyright__Link(href="http://www.asce.org" itemprop="url").
            &copy; #[time 1996]&ndash;#[time 2016], #[span(itemprop="name") American Society of Civil Engineers]
