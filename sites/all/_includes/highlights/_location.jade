div.c-Overlay.c-Overlay--callout
  -
    var loc = site.getCurrentConference().location
    var heading = loc.addressLocality + ', ' + loc.addressRegion
  h1(id=Util.toURL(heading))= heading
  p: block blurb
  +highlightButtons([{text:'Hotel & Travel', href:'location.html'}], 'c-Btn--ghost c-Btn--ghost--dark')
